[alias]
  co = checkout
  br = branch
  cm = commit
  cp = cherry-pick
  pullr = pull --rebase
  com = checkout master
  cop = checkout production
  hardreset = reset --hard HEAD^
  mm = merge --no-ff master
  pm = push origin master
  pp = push origin production
  ada = add .
  dfc = diff --cached
  st = status

